    "@sentry/vue": "^7.1.1",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.0.14",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@graphql-codegen/cli": "^2.6.2",
